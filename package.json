{
  "name": "experiment-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=development && nodemon ./bin/www",
    "babel-build-scripts": "./node_modules/.bin/babel build-scripts --out-dir build-scripts-es5",
    "copy-views": "node build-scripts/copy-views.js",
    "copy-assets": "node build-scripts/copy-assets.js",
    "copy-files-build": "npm run copy-views && npm run copy-assets",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "cookie-parser": "~1.4.3",
    "createjs": "^1.0.1",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/createjs": "0.0.29",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.3",
    "@types/node-sass-middleware": "0.0.30",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "fs-extra": "^6.0.1",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
